{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tauri-sqlite-vss-demo",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri-sqlite-vss-demo.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "jieba/jieba.dict.utf8",
        "jieba/user.dict.utf8",
        "jieba/hmm_model.utf8",
        "jieba/idf.utf8",
        "jieba/stop_words.utf8"
      ],
      "macOS": {
        "frameworks": [
          "./libs/x86_64/libomp.dylib"
        ]
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-sqlite-vss-demo",
        "width": 800,
        "height": 600
      }
    ]
  }
}
